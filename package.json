{
  "name": "beathub",
  "version": "0.1.0",
  "description": "Beathub",
  "main": "index.js",
  "author": "smbsol <info@smbsol.com>",
  "license": "MIT",
  "build": {
    "icon": "static/favicon.ico",
    "productName": "Beathub",
    "files": [
      "build/**/*",
      "main.js"
    ],
    "appId": "com.smbsol.beathub",
    "win": {},
    "linux": {
      "category": "Utility",
      "icon": "build/favicon.png",
      "target": "deb"
    },
    "mac": {}
  },
  "dependencies": {
    "electron-serve": "^1.0.0",
    "fs": "^0.0.1-security",
    "lokijs": "^1.5.12",
    "music-metadata": "^7.13.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "app-settings-loader": "^2.0.7",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.4.0",
    "aurelia-cli": "^3.0.1",
    "aurelia-dialog": "^2.0.0",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-loader-nodejs": "^1.1.0",
    "aurelia-pal-nodejs": "^2.0.0",
    "aurelia-testing": "^1.1.0",
    "aurelia-webpack-plugin": "^5.0.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "electron": "^27.3.6",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.30.0",
    "expose-loader": "^4.0.0",
    "file-loader": "^6.0.0",
    "force-graph": "^1.42.9",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "jest-transform-stub": "^2.0.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.7.2",
    "minimatch": "^5.1.2",
    "moment": "^2.29.4",
    "node-sass": "^6",
    "postcss-loader": "^4",
    "promise-polyfill": "^8.2.3",
    "query-string": "^8.1.0",
    "regenerator-runtime": "^0.13.11",
    "repeat": "^2.0.4",
    "sass-loader": "^10",
    "style-loader": "^3.3.1",
    "tippy.js": "^6.3.7",
    "toastr": "^2.1.4",
    "tree-kill": "^1.2.2",
    "url-loader": "^4.1.0",
    "wait-on": "^5.2.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "build": "webpack --env.production",
    "start": "webpack-dev-server",
    "build:dev": "webpack",
    "analyze": "webpack --env.production --analyze",
    "test": "au test",
    "electron": "wait-on http://localhost:9090 && electron .",
    "electron-dev": "concurrently \"yarn run start\" \"yarn run electron\"",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^aurelia-binding$": "<rootDir>/node_modules/aurelia-binding"
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(css|less|sass|scss|styl|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "\\.spec\\.js$",
    "setupFiles": [
      "<rootDir>/test/jest-pretest.js"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/*.spec.js",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "keywords": [
    "cross-platform",
    "native-app",
    "installed-app",
    "desktop-applications",
    "windows",
    "linux",
    "macos",
    "html",
    "css",
    "javascript",
    "aurelia",
    "typescript",
    "pwa",
    "spa",
    "web-performance",
    "electron",
    "electron-builder",
    "electron-serve",
    "concurrently",
    "wait-on"
  ]
}
